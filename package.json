{
    "name": "enjin-theme",
    "description": "Responsive Enjin theme for the Really Bad Players community.",
    "repository": {
        "type": "git",
        "url": "https://github.com/Seikcud/enjin-theme.git"
    },
    "scripts": {
        "build": "SET NODE_ENV=production&& gulp build --require @babel/register --gulpfile tasks",
        "develop": "SET NODE_ENV=development&& gulp watch --require @babel/register --gulpfile tasks",
        "watch": "SET NODE_ENV=production&& gulp watch --require @babel/register --gulpfile tasks"
    },
    "devDependencies": {
        "@babel/core": "7.1.2",
        "@babel/preset-env": "7.1.0",
        "@babel/register": "7.0.0",
        "babel-loader": "8.0.4",
        "basiclightbox": "4.0.3",
        "browser-sync": "2.26.3",
        "browser-sync-webpack-plugin": "2.2.2",
        "eslint": "5.8.0",
        "browserslist": "4.3.4",
        "eslint-config-google": "0.11.0",
        "eslint-plugin-compat": "2.6.3",
        "flickity": "2.1.2",
        "flickity-bg-lazyload": "1.0.0",
        "gulp": "4.0.0",
        "gulp-autoprefixer": "6.0.0",
        "gulp-babel": "8.0.0",
        "gulp-better-rollup": "3.3.0",
        "gulp-clean-css": "3.10.0",
        "gulp-concat": "2.6.1",
        "gulp-if": "2.0.2",
        "gulp-rename": "1.4.0",
        "gulp-sass": "4.0.2",
        "gulp-sourcemaps": "2.6.4",
        "gulp-uglify": "3.0.1",
        "imagesloaded": "4.1.4",
        "stylelint-config-standard": "18.2.0",
        "stylelint-no-unsupported-browser-features": "3.0.2",
        "stylelint-order": "1.0.0",
        "stylelint-scss": "3.3.1",
        "stylelint-selector-bem-pattern": "2.0.0",
        "webpack": "4.25.1"
    },
    "dependencies": {
        "material-components-web": "0.41.0",
        "npm": "6.4.1"
    },
    "sasslintConfig": "./.stylelintrc.yaml",
    "browserslist": "> 5% in US, not dead",
    "renovate": {
        "extends": [
            "config:base"
        ]
    }
}
